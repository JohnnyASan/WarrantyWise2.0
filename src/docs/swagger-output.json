{
  "swagger": "2.0",
  "info": {
    "title": "Warranties API",
    "description": "Api used for saving, updated, reading, and deleting warranties.",
    "version": "1.0.0"
  },
  "host": "warrantywise.onrender.com",
  "basePath": "/",
  "schemes": [
    "https"
  ],
  "securityDefinitions": {
    "Bearer": {
      "type": "apiKey",
      "name": "Authorization",
      "in": "header",
      "description": "Enter the token with the `Bearer: ` prefix, e.g. \"Bearer abcde12345\"."
    }
  },
  "paths": {
    "/api-docs": {
      "get": {
        "description": "",
        "responses": {
          "default": {
            "description": ""
          }
        }
      }
    },
    "/warranties/": {
      "get": {
        "tags": [
          "Warranties"
        ],
        "summary": "Get All Warranties",
        "description": "Gets all warranties in the collection. This endpoint is NOT paginated.",
        "produces": [
          "application/json"
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      },
      "post": {
        "tags": [
          "Warranties"
        ],
        "summary": "Create a new Warranty",
        "description": "Creates a new warranty in the warranties collection.",
        "parameters": [
          {
            "name": "body",
            "in": "body",
            "description": "Create a new warranty",
            "schema": {
              "type": "object",
              "properties": {
                "modelNumber": {
                  "type": "string",
                  "example": "123MODEL"
                },
                "purchaseDate": {
                  "type": "string",
                  "example": "2024/10/19"
                },
                "expiration": {
                  "type": "string",
                  "example": "2024/10/19"
                },
                "company": {
                  "type": "string",
                  "example": "WarrantyWise"
                },
                "details": {
                  "type": "string",
                  "example": "Full coverage for any reason"
                },
                "email": {
                  "type": "string",
                  "example": "john.doe@email.com"
                },
                "phone": {
                  "type": "string",
                  "example": "1234567890"
                },
                "linkToFileClaim": {
                  "type": "string",
                  "example": "file.claim.com"
                },
                "githubId": {
                  "type": "string",
                  "example": "123ID"
                }
              }
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Create a new warranty.",
            "schema": {
              "type": "object",
              "properties": {
                "modelNumber": {
                  "type": "string",
                  "example": "123MODEL"
                },
                "purchaseDate": {
                  "type": "string",
                  "example": "2024/10/19"
                },
                "expiration": {
                  "type": "string",
                  "example": "2024/10/19"
                },
                "company": {
                  "type": "string",
                  "example": "WarrantyWise"
                },
                "details": {
                  "type": "string",
                  "example": "Full coverage for any reason"
                },
                "email": {
                  "type": "string",
                  "example": "john.doe@email.com"
                },
                "phone": {
                  "type": "string",
                  "example": "1234567890"
                },
                "linkToFileClaim": {
                  "type": "string",
                  "example": "file.claim.com"
                },
                "githubId": {
                  "type": "string",
                  "example": "123ID"
                }
              },
              "xml": {
                "name": "main"
              }
            }
          },
          "201": {
            "description": "Created"
          },
          "500": {
            "description": "Internal Server Error"
          }
        }
      }
    },
    "/warranties/{id}": {
      "get": {
        "tags": [
          "Warranties"
        ],
        "summary": "Get Warranty By ID",
        "description": "Gets a single warranty by the provided ID.",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "type": "string",
            "description": "ID of the warranty to be retrieved."
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      },
      "put": {
        "tags": [
          "Warranties"
        ],
        "summary": "Update a Warranty by ID",
        "description": "Updated a warranty in the warranties collection by provided ID.",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "type": "string",
            "description": "ID of the warranty to be updated."
          },
          {
            "name": "body",
            "in": "body",
            "description": "Update a warranty",
            "schema": {
              "type": "object",
              "properties": {
                "modelNumber": {
                  "type": "string",
                  "example": "123MODEL"
                },
                "purchaseDate": {
                  "type": "string",
                  "example": "2024/10/19"
                },
                "expiration": {
                  "type": "string",
                  "example": "2024/10/19"
                },
                "company": {
                  "type": "string",
                  "example": "WarrantyWise"
                },
                "details": {
                  "type": "string",
                  "example": "Full coverage for any reason"
                },
                "email": {
                  "type": "string",
                  "example": "john.doe@email.com"
                },
                "phone": {
                  "type": "string",
                  "example": "1234567890"
                },
                "linkToFileClaim": {
                  "type": "string",
                  "example": "file.claim.com"
                },
                "githubId": {
                  "type": "string",
                  "example": "123ID"
                }
              }
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Update a warranty.",
            "schema": {
              "type": "object",
              "properties": {
                "modelNumber": {
                  "type": "string",
                  "example": "123MODEL"
                },
                "purchaseDate": {
                  "type": "string",
                  "example": "2024/10/19"
                },
                "expiration": {
                  "type": "string",
                  "example": "2024/10/19"
                },
                "company": {
                  "type": "string",
                  "example": "WarrantyWise"
                },
                "details": {
                  "type": "string",
                  "example": "Full coverage for any reason"
                },
                "email": {
                  "type": "string",
                  "example": "john.doe@email.com"
                },
                "phone": {
                  "type": "string",
                  "example": "1234567890"
                },
                "linkToFileClaim": {
                  "type": "string",
                  "example": "file.claim.com"
                },
                "githubId": {
                  "type": "string",
                  "example": "123ID"
                }
              },
              "xml": {
                "name": "main"
              }
            }
          },
          "204": {
            "description": "No Content"
          }
        }
      },
      "delete": {
        "tags": [
          "Warranties"
        ],
        "summary": "Delete a Warranty by ID",
        "description": "Deletes a warranty from the warranties collection for the provided ID.",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "type": "string",
            "description": "ID of the warranty to be deleted."
          }
        ],
        "responses": {
          "204": {
            "description": "No Content"
          }
        }
      }
    },
    "/auth/": {
      "get": {
        "description": "",
        "responses": {
          "default": {
            "description": ""
          }
        }
      }
    },
    "/auth/callback": {
      "get": {
        "description": "",
        "responses": {
          "default": {
            "description": ""
          }
        }
      }
    },
    "/auth/success": {
      "get": {
        "description": "",
        "responses": {
          "default": {
            "description": ""
          }
        }
      }
    }
  }
}